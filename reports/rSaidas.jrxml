		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
f.s_cpf,(SELECT p.pc_nome FROM pacientes p WHERE CAST(p.pc_numero AS Text) = f.s_cpf OR CAST(p.pc_cpf AS Text) = f.s_cpf) AS s_paciente,f.s_codigo,f.s_descricao,f.s_dtsaida,f.s_unidade, f.s_quantidade
FROM
s_farmacia f
WHERE
f.s_dtsaida >= Cast($P{dtini} AS Date) AND f.s_dtsaida <= Cast($P{dtfim} AS Date)
ORDER BY
f.s_cpf,
Upper(f.s_descricao),
f.s_dtsaida]]>
	</queryString>
